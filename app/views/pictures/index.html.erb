<p id="notice"><%= notice %></p>

<h1>Pictures</h1>
<%= link_to 'New Picture', new_picture_path %>
<table>
  <thead>
    <tr>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<div class="feed_page">
  <div class="feed_title"><%= page_entries_info @pictures %></div>
  <div class="feed">
    <%= paginate @pictures %>
  </div>
</div>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>
        <td><%= image_tag(picture.url + 'media/?size=t') %></td>
        <td><%= link_to 'Show', picture %></td>
        <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
        <td><%= link_to 'Destroy', picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="feed_page">
  <div class="feed_title"><%= page_entries_info @pictures %></div>
  <div class="feed">
    <%= paginate @pictures %>
  </div>
</div>

<br>
